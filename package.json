{
  "name": "uploader",
  "version": "0.0.1",
  "description": "Serverless File Uploader",
  "main": "handler.js",
  "scripts": {
    "test": "jest --testPathIgnorePatterns /node_modules/ --",
    "lint": "eslint '{,!(node_modules)/**/}*.js' *.js",
    "lint-fix": "eslint '{,!(node_modules)/**/}*.js' --fix",
    "fmt": "prettier --write '{,!(node_modules|coverage)/**/}*.js'",
    "deploy": "sls deploy -f"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sadasant/uploader-js.git"
  },
  "author": "Daniel Rodriguez (sadasant@gmail.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sadasant/uploader-js/issues"
  },
  "homepage": "https://github.com/sadasant/uploader-js#readme",
  "devDependencies": {
    "aws-sdk-mock": "^4.0.0",
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.0.1",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-plugin-jest": "^21.17.0",
    "jest": "^23.1.0",
    "jest-cli": "^23.1.0",
    "prettier": "^1.13.5",
    "regenerator-runtime": "^0.11.1",
    "serverless-webpack": "^5.1.5",
    "source-map-support": "^0.5.6",
    "webpack": "^4.12.0"
  },
  "babel": {
    "presets": [
      "env",
      "stage-2",
      "stage-3"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/__tests__/setup.js"
    ],
    "testMatch": [
      "<rootDir>/**/*.test.js"
    ],
    "collectCoverage": true
  },
  "dependencies": {
    "@aws/dynamodb-data-mapper": "^0.6.0",
    "@aws/dynamodb-expressions": "^0.6.0",
    "@aws/dynamodb-query-iterator": "^0.6.0",
    "aws-sdk": "^2.262.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "crypto-promise": "^2.1.0",
    "email-validator": "^2.0.4",
    "node-uuid": "^1.4.8"
  }
}
